      <div id="warehouses-container" class="div-block-54">
        <div class="columns-9 w-row">
          <div class="column-19 active w-col w-col-4">
            <div class="warehouse-sort-title">Location</div>
          </div>
          <div class="column-19 w-col w-col-3">
            <div class="warehouse-sort-title">City</div>
          </div>
          <div class="column-19 w-col w-col-3">
            <div class="warehouse-sort-title">Price</div>
          </div>
          <div class="column-19 w-col w-col-2">
            <div class="warehouse-sort-title">Size</div>
          </div>
        </div>
        <div class="collection-list-wrapper-2 w-dyn-list">
          
          <% for (i = 0; i < count; i++) { %>
              <div class="warehouses-list w-dyn-items">
                <div class="item w-dyn-item">
                  <a href="#" class="link-block-8 w-inline-block">
                    <div class="warehouse-row w-row">
                      <div class="w-col w-col-4">
                        <div class="longitude"><%= data[i]['longitude-2'] %></div>
                        <div class="latitude"><%= data[i]['latitud-2'] %></div>
                        <div class="warehouse-location"><%= data[i]['name'] %></div>
                      </div>
                      <div class="w-col w-col-3">
                        <div class="warehouse-city"><%= data[i]['city'] + ', ' + data[i]['state'] %></div>
                      </div>
                      <div class="w-col w-col-3">
                        <div class="div-block-53">
                          <div class="text-block-282 symbol">$</div>
                          <div class="warehouse-price">10</div>
                          <div class="text-block-282 side">/ day</div>
                        </div>
                      </div>
                      <div class="column-18 w-col w-col-2">
                        <div class="warehouse-size"><%= data[i]['measurement'] %></div>
                        <div class="text-block-281">sq feet</div>
                      </div>
                    </div>
                  </a>
                </div>
              </div>
          <% } %>

          <div class="w-pagination-wrapper pagination-2">

            <% if(page_count > 1){ %>
            <form method="post" action="/">
              <input type="hidden" name="req_type" value="prv">  
              <button type="submit" style="background: none;"><a id="prv" href="#" class="w-pagination-previous previous"><svg class="w-pagination-previous-icon" height="12px" width="12px" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 12 12" transform="translate(0, 1)"><path fill="none" stroke="currentColor" fill-rule="evenodd" d="M8 10L4 6l4-4"></path></svg><div class="w-inline-block">Previous</div></a></button>
            </form>
            <% } %>

            <% if(page_count < total_pages){ %>
            <form method="post" action="/">
              <input type="hidden" name="req_type" value="nxt">
              <button type="submit" style="background: none;"><a id="nxt" href="#" class="w-pagination-next next">
                <div class="w-inline-block">Next</div><svg class="w-pagination-next-icon" height="12px" width="12px" xmlns="http://www.w3.org/2000/svg" viewbox="0 0 12 12" transform="translate(0, 1)"><path fill="none" stroke="currentColor" fill-rule="evenodd" d="M4 2l4 4-4 4"></path></svg></a></button>
            </form>
            <% } %>

            <div id="page_num" class="w-pagination-page-count page-count"><%= page_count.toString() + '/' + total_pages.toString() %></div>
          </div> 
        </div>
      </div>

    </div>
